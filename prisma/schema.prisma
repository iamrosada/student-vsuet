generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"

  url = "file:./database.db"
}

model Student {
  id        Int    @id @default(autoincrement())
  email     String @unique
  telephone String @unique
  IdStudent String @unique
  firstName String
  lastName  String
  password  String
}
