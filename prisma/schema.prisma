generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"

  url = "file:./database.db"
}

model Student {
  id              Int     @id @default(autoincrement())
  email           String  @unique
  telephone       String  @unique
  IdStudent       String  @unique
  firstName       String
  lastName        String
  ageStart        String
  ageFinish       String
  country         String
  language        String
  whereLive       String
  fullNameInLatin String
  diplomIsRed     Boolean

  whereDidThePreparatory String
  group                  String
  course                 String
}

model Languange {
  id       Int    @id @default(autoincrement())
  language String
}
